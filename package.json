{
  "name": "@surikat/updf",
  "version": "0.4.2",
  "description": "Hopefully tiny and modular library to generate PDF's in JS",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive test",
    "test.watch": "mocha --watch --color --compilers js:babel-register --recursive test",
    "compile": "babel -d lib src",
    "compile.watch": "babel --watch -d lib src",
    "prepublish": "npm run compile"        
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "eslint": "^2.6.0",
    "eslint-config-surikat": "^1.1.0",
    "mocha": "^2.4.5",
    "should": "^8.3.0"
  },
  "keywords": [
    "PDF",
    "JS"
  ],
  "author": "Surikat AB",
  "license": "MIT"
}
